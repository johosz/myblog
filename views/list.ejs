<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <link rel="stylesheet" href="/bootstrap/css/bootstrap.css">
    <script src="/javascripts/jquery-3.6.0.js"></script>
    <script src="/bootstrap/js/bootstrap.js"></script>

</head>
<body>

<div class="container">

    <div class="row">
        <div class="col">
            <div class="card mt-1">

                <% data.forEach(item=>{ %>

                    <div class="card-header">

                        <a href="/blog/detail?id=<%= item.id %>"> <%= item.title %></a>

                    </div>
                <% }) %>
            </div>

        </div>

    </div>
</div>


</body>
</html>